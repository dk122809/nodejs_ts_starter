{
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "next"
            }
        ],
        "no-use-before-define": [
            "error",
            {
                "variables": false
            }
        ],
        "no-multi-str": 0
    }
}